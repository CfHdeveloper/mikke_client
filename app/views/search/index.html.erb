<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <p>search</p>
    <p>genre : <%= @genre %></p>
  
    <% @circles['circles'].each do |circle| %>
    <div>
      <ul>
        <% circle.each do |key,value| %>
        <li><%= key %> : <%= value %></li>
        <% end %>
      </ul>
    </div>
    <% end %>
    <a href="/circles/hoge">jump</a>
  </body>
</html>
